+exitY = rows-1
+monsterX = cols-1
+monsterY = rows-2
+
+render()
+
+
+// MOVEMENT ------------------------------------------------
+
+move(dir):
+if gameOver ‚Üí return
+
+nx = playerX
+ny = playerY
+
+if dir == "Up": ny -= 1
+if dir == "Down": ny += 1
+if dir == "Left": nx -= 1
+if dir == "Right": nx += 1
+
+if nx < 0 or ny < 0 or nx >= cols or ny >= rows:
+  return
+
+if maze[ny][nx] == 1:
+  return
+
+// smooth animation
+animStep = 0
+animatePlayer(nx, ny)
+
+animatePlayer(nx, ny):
+if animStep < animMax:
+  animStep += 1
+  render()
+  return
+
+playerX = nx
+playerY = ny
+
+if playerX == exitX and playerY == exitY:
+  endGame(true)
+  return
+
+monsterChase()
+render()
+
+
+// MONSTER AI ---------------------------------------------
+
+monsterChase():
+dx = playerX - monsterX
+dy = playerY - monsterY
+
+options = []
+
+if abs(dx) > abs(dy):
+  options.push([monsterX + sign(dx), monsterY])
+  options.push([monsterX, monsterY + sign(dy)])
+else:
+  options.push([monsterX, monsterY + sign(dy)])
+  options.push([monsterX + sign(dx), monsterY])
+
+// fallback
+options.push([monsterX+1, monsterY])
+options.push([monsterX-1, monsterY])
+options.push([monsterX, monsterY+1])
+options.push([monsterX, monsterY-1])
+
+for each op in options:
+  ox = op[0]
+  oy = op[1]
+  if ox >= 0 and oy >= 0 and ox < cols and oy < rows:
+    if maze[oy][ox] == 0:
+       monsterX = ox
+       monsterY = oy
+       break
+
+if monsterX == playerX and monsterY == playerY:
+  endGame(false)
+
+
+// ENDGAME ----------------------------------------------
+
+endGame(win):
+gameOver = true
+if win:
+  message = "üèÜ You escaped!"
+else:
+  message = "üíÄ The monster caught you!"
+render()
+
+
+
