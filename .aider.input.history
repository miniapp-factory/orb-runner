+    else if maze[y][x]==0 then cell.backgroundColor="black"
+    else cell.backgroundColor="white"
+    appendToGridContainer(cell)
+
+move(dir):
+// check bordi e muri
+// aggiorna playerX/Y
+moves+=1; timeLeft-=1
+checkWin()
+monsterMove()
+checkMonsterCaught()
