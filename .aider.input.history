+initGame():
+rows = 10
+cols = 10
+gameOver = false
+animStep = 0
+animMax = 2
+message = "Escape the maze!"
+
+// Generate empty maze
+maze = []
+for y 0..rows-1:
+  row = []
+  for x 0..cols-1:
+    row.push(1)
+  maze.push(row)
+
+// Guaranteed path carving
+px = 0
+py = 0
+maze[py][px] = 0
+while px != cols-1 or py != rows-1:
+  if random() < 0.5 and px < cols-1:
+    px += 1
+  else if py < rows-1:
+    py += 1
+  maze[py][px] = 0
+
+// Add random openings
+for i 0..40:
+  rx = floor(random()*cols)
+  ry = floor(random()*rows)
+  maze[ry][rx] = 0
